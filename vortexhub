	end)
	
end
local function IJOQ_fake_script() -- Fake Script: Workspace.ScreenGui.keysys.title.TextLabel.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_TextLabel8"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local text = script.Parent
	local add = 10
	wait(1)
	local k = 1
	while k <= 255 do
		text.TextColor3 = Color3.new(k/255,0/255,0/255)
		k = k + add
		wait()
	end
	while true do
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255,k/255,0/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255 - k/255,255/255,0/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(0/255,255/255,k/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(0/255,255/255 - k/255,255/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(k/255,0/255,255/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255,0/255,255/255 - k/255)
			k = k + add
			wait()
		end
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255 - k/255,0/255,0/255)
			k = k + add
			wait()
		end
	end
end
local function FVUJF_fake_script() -- Fake Script: Workspace.ScreenGui.keysys.submit.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_submit"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	enterkey = script.Parent.Parent.keybox
	submit = script.Parent
	keylogin = script.Parent.Parent.Parent.keysys
	mainf = script.Parent.Parent.Parent.mainhold
	open = script.Parent.Parent.Parent.open
	
	submit.MouseButton1Click:Connect(function()
		if enterkey.Text == "VORTEX!lol" then
			game.StarterGui:SetCore("SendNotification", {
				Title = "VORTEX KEY SYSTEM!";
				Text = "Correct Key."
			})
			keylogin.Visible = false
			wait(0.5)
			mainf.Visible = true
			open.Visible = true
		else
			game.StarterGui:SetCore("SendNotification", {
				Title = "VORTEX KEY SYSTEM!";
				Text = "Incorrect Key."
			})
		end
	end)
end
local function FETR_fake_script() -- Fake Script: Workspace.ScreenGui.keysys.clipboard.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_clipboard"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	script.Parent.MouseButton1Click:Connect(function()
		setclipboard("https://discord.gg/HkjCCjnaPz")
		game.StarterGui:SetCore("SendNotification", {
			Title = "VORTEX KEY SYSTEM";
			Text = "Check your clipboard (WIN+V)"
		})
	end)
end
