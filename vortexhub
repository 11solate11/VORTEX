local function HQYAS_fake_script() -- Fake Script: Workspace.ScreenGui.mainhold.thold.fun.back.backscript
    local script = Instance.new("LocalScript")
    script.Name = "backscript"
    script.Parent = Converted["_back"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	--// put all of that in a mouse button click function
	script.Parent.MouseButton1Click:Connect(function()
		--// hides all tabs
		local tabs = script.Parent.Parent.Parent.Parent.thold
		for i, v in pairs(tabs:GetChildren()) do
			if v:IsA("Frame") then
				v.Visible = false
			end
		end
	
		--// shows tab u want
		script.Parent.Parent.Parent.Parent.thold.home.Visible = true
		--                                /\ PUT TAB NAME THERE	
	end)
end
