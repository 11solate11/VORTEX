local function PDNXD_fake_script() -- zbloxfruits.color 
	local script = Instance.new('LocalScript', zbloxfruits)

	-- LocalScript inside the TextButton
	
	local button = script.Parent
	local TweenService = game:GetService("TweenService")
	
	-- Store the original size, position, and text color
	local originalSize = button.Size
	local originalPosition = button.Position
	local originalTextColor = button.TextColor3
	
	-- Define tween information for the bounce effect
	local tweenInfo = TweenInfo.new(
		0.1, -- Time
		Enum.EasingStyle.Quad, -- EasingStyle
		Enum.EasingDirection.Out -- EasingDirection
	)
	
	-- Define the size increase and position offset for the bounce effect
	local hoverSizeIncrease = UDim2.new(0, 5, 0, 5)
	local goalSizeIncrease = UDim2.new(0, 2, 0, 2) -- Slight bounce increase
	local goalPositionOffset = UDim2.new(0, -1, 0, -1) -- Slight position offset
	
	-- Variable to keep track of current tween
	local currentTween
	local runningRainbow = false
	
	-- Create tween function
	local function createTween(goal)
		return TweenService:Create(button, tweenInfo, goal)
	end
	
	-- Function to play bounce effect
	local function playBounceEffect()
		if currentTween then
			currentTween:Cancel()
		end
	
		local goal = {Size = originalSize + hoverSizeIncrease, Position = originalPosition + goalPositionOffset}
		currentTween = createTween(goal)
		currentTween:Play()
		currentTween.Completed:Connect(function()
			local goal = {Size = originalSize + goalSizeIncrease, Position = originalPosition + goalPositionOffset}
			currentTween = createTween(goal)
			currentTween:Play()
		end)
	end
	
	-- Function to play rainbow effect
	local function playRainbowEffect()
		local colors = {
			Color3.fromRGB(255, 0, 0),    -- Red
			Color3.fromRGB(255, 127, 0),  -- Orange
			Color3.fromRGB(255, 255, 0),  -- Yellow
			Color3.fromRGB(0, 255, 0),    -- Green
			Color3.fromRGB(0, 0, 255),    -- Blue
			Color3.fromRGB(75, 0, 130),   -- Indigo
			Color3.fromRGB(148, 0, 211)   -- Violet
		}
	
		local colorIndex = 1
		runningRainbow = true
	
		while runningRainbow do
			local nextColor = colors[colorIndex]
			local colorTween = TweenService:Create(button, tweenInfo, {TextColor3 = nextColor})
			colorTween:Play()
	
			colorTween.Completed:Wait()
			colorIndex = (colorIndex % #colors) + 1
		end
	end
	
	-- Hover effect
	button.MouseEnter:Connect(function()
		if currentTween then
			currentTween:Cancel()
		end
	
		playBounceEffect()
		playRainbowEffect()
	end)
	
	button.MouseLeave:Connect(function()
		if currentTween then
			currentTween:Cancel()
		end
		runningRainbow = false -- Stop the rainbow effect
	
		local goal = {Size = originalSize, Position = originalPosition, TextColor3 = originalTextColor}
		currentTween = createTween(goal)
		currentTween:Play()
	end)
	
end
coroutine.wrap(PDNXD_fake_script)()
local function ARYNQRL_fake_script() -- universal.searchscript 
	local script = Instance.new('LocalScript', universal)

	local scroll = script.Parent.uniscroll
	local textBox = script.Parent.bsearch
	
	textBox.Changed:Connect(function() -- when the text is changed
		local text = textBox.Text:lower() -- lowercase search bar text
		if text ~= "" then -- if it has text
			local buttons = scroll:GetDescendants() -- all of the buttons
			for _, button in pairs(buttons) do -- loops through the buttons
				if button:IsA("TextButton") then -- if it's a button
					local buttonText = button.Text:lower() -- lowercase button text
					if string.find(buttonText, text) then -- if search bar text is found in the button's text
						button.Visible = true -- shows button
					else -- otherwise
						button.Visible = false -- hides button
					end
				end
			end
		else -- if it's empty
			local buttons = scroll:GetDescendants() -- all buttons
			for _, button in pairs(buttons) do -- loops through buttons
				if button:IsA("TextButton") then -- if it's a button
					button.Visible = true -- shows button
				end
			end
		end
	end)
end
coroutine.wrap(ARYNQRL_fake_script)()
local function TTLAL_fake_script() -- atitle.LocalScript 
	local script = Instance.new('LocalScript', atitle)

	local text = script.Parent
	local add = 10
	wait(1)
	local k = 1
	while k <= 255 do
		text.TextColor3 = Color3.new(k/255,0/255,0/255)
		k = k + add
		wait()
	end
	while true do
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255,k/255,0/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255 - k/255,255/255,0/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(0/255,255/255,k/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(0/255,255/255 - k/255,255/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(k/255,0/255,255/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255,0/255,255/255 - k/255)
			k = k + add
			wait()
		end
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255 - k/255,0/255,0/255)
			k = k + add
			wait()
		end
	end
end
coroutine.wrap(TTLAL_fake_script)()
